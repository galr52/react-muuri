    "jest": "^25.2.6",
    "muuri": "^0.8.0",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.3.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "muuri": "^0.8.0",
    "react": "^17.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
